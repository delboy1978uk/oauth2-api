{
    "name": "delboy1978uk/oauth2-api",
    "description": "An OAuth2 API built using Bone MVC Framework and the Extraordinary League's OAuth2 Server",
    "authors": [
        {
            "name": "Derek McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "delboy1978uk/bone": "dev-dev-master",
        "delboy1978uk/cdn": "^1.1.2",
        "delboy1978uk/form": "^1.0.0",
        "delboy1978uk/image": "^1.0.0",
        "delboy1978uk/session": "^1.0",
        "delboy1978uk/user": "^2.0",
        "league/oauth2-server": "^5.1",
        "zircote/swagger-php": "^2.0",
        "jms/serializer": "^1.11"
    },

    "require-dev": {
        "codeception/codeception": "*",
        "codeception/aspect-mock": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "BoneMvc\\": "src/BoneMvc/",
            "OAuth\\": "src/Entity/OAuth/",
            "OAuthTest\\": "tests/unit/OAuth/"
        }
    }
}
